<h1><%= @occasion.name%></h1>

<%= form_for(@outfit, url: occasion_outfits_path(@occasion)) do |f| %>
	<p>
		<%= f.label :name%><br />
		<%= f.text_field :name%>
	</p>
	<%= f.submit %>
<% end %>

<%= link_to "edit", edit_occasion_path(@occasion)%>

<%= link_to "destroy", @occasion, method: :delete, data: "are you sure you want to delete?" %>